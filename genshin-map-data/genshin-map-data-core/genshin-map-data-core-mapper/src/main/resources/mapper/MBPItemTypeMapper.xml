<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="site.yuanshen.data.mapper.ItemTypeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="site.yuanshen.data.entity.ItemType">
        <id column="id" property="id"/>
        <id column="parent_id" property="parentId"/>
        <id column="sort_index" property="sortIndex"/>
        <result column="creator_id" property="creatorId"/>
        <result column="create_time" property="createTime"/>
        <result column="del_flag" property="delFlag"/>
        <result column="version" property="version"/>
        <result column="updater_id" property="updaterId"/>
        <result column="update_time" property="updateTime"/>
        <result column="icon_tag" property="iconTag"/>
        <result column="name" property="name"/>
        <result column="content" property="content"/>
        <result column="is_final" property="isFinal"/>
        <result column="hidden_flag" property="hiddenFlag"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        it.creator_id,
        it.create_time,
        it.del_flag,
        it.version,
        it.id,
        it.updater_id,
        it.update_time,
        it.icon_tag,
        it.name,
        it.content,
        it.parent_id,
        it.is_final,
        it.hidden_flag,
        it.sort_index
    </sql>

</mapper>
